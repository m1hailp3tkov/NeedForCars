@model IEnumerable<IGrouping<char, DisplayMakeModel>>

@foreach (var group in Model)
{
    <h4>@group.Key</h4>
    <hr/>
    <div class="row">
        @foreach (var make in group)
        {
        <div class="col-sm-3">
            <a href="">
                <img src="@make.ImagePath" width="100" height="100" />
                <b>@make.Name</b>
            </a>
        </div>
        }
    </div>
}
